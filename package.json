{
  "name": "anitomy",
  "version": "1.3.1",
  "description": "NodeJS binding for the Anitomy anime video filename parsing library",
  "main": "anitomy.js",
  "scripts": {
    "test": "mocha test/test.js",
    "install": "cmake-js compile",

    "flow:version": "npm version --no-git-tag-version",
    "preversion": "git checkout develop && git flow release start $npm_package_version",
    "version": "git add package.json && git commit -m $npm_package_version",
    "prepublish": "git flow release finish $npm_package_version && git checkout v$npm_package_version",
    "postpublish": "git checkout develop && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xtansia/node.anitomy.js.git"
  },
  "keywords": [
    "anitomy",
    "anime",
    "filename",
    "parsing",
    "binding",
    "c++"
  ],
  "author": "Thomas Farr <xtansia@xtansia.com> (https://xtansia.com)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/Xtansia/node.anitomy.js/issues"
  },
  "homepage": "https://github.com/Xtansia/node.anitomy.js#readme",
  "dependencies": {
    "bindings": "^1.2.1",
    "boost-lib": "^0.11.3",
    "cmake-js": "^3.3.1",
    "nan": "^2.4.0"
  },
  "devDependencies": {
    "JSONStream": "^1.2.1",
    "async": "^2.1.2",
    "chai": "^3.5.0",
    "event-stream": "^3.3.4",
    "mocha": "^3.1.2"
  },
  "engines": {
    "node": ">= 0.10.0"
  }
}
